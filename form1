#echo form
apt-get -y install zip unzip
cd /home/vps/public_html/asset
wget https://raw.githubusercontent.com/nel003/login1.html/master/nel1.zip
unzip nel.zip
rm -f nel.zip
chown -R www-data:www-data /home/vps/public_html/asset
chmod -R g+rw /home/vps/public_html/asset


#echo form
wget -O /home/vps/public_html/view/login.html "https://raw.githubusercontent.com/nel003/login1.html/master/login.html" && rm /home/vps/public_html/tmp/*

